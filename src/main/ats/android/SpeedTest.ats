[ats-variables]

var -> ip_port -> $env(android_ip_port)
var -> ping -> regexp [Ping (.*) (.*), 1]
var -> gigue -> regexp [Gigue (.*) (.*), 1]
var -> perte -> regexp [Perte (.*) (.*), 1]
var -> today -> date [dd/MM/yy] -> $today

[ats-actions]

