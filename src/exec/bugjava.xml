<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="bugjava" verbose="10">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="2"/>
  <parameter name="ats.log.level" value="all"/>
  <test name="testMain" preserve-order="true">
    <parameter name="android_ip_port" value="192.168.0.19:8080"/>
    <classes>
      <class name="web.subscripts.Web_chrome"/>
      <class name="web.subscripts.Web_firefox"/>
      <class name="web.subscripts.Web_ie"/>
      <class name="web.subscripts.Web_msedge"/>
      <class name="web.subscripts.Web_opera"/>
    </classes>
  </test>
</suite>