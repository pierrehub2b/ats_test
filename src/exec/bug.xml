<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="bug" verbose="1">
  <test name="testMain">
    <groups>
      <run>
        <include name="bug"/>
        <exclude name="default"/>
        <exclude name="toverify"/>
        <exclude name="web"/>
        <exclude name="desktop"/>
      </run>
    </groups>
    <packages>
      <package name=".*"/>
      <package name="channel.*"/>
      <package name="check.*"/>
      <package name="desktop.*"/>
      <package name="inputs.*"/>
      <package name="mouse.*"/>
      <package name="other.*"/>
      <package name="subscripts.*"/>
      <package name="subscripts.channel.*"/>
      <package name="subscripts.data.*"/>
      <package name="subscripts.dragdrop.*"/>
      <package name="subscripts.inputs.*"/>
      <package name="subscripts.mouse.*"/>
      <package name="subscripts.mouse.scroll.*"/>
      <package name="subscripts.window.*"/>
    </packages>
    <classes>
      <class name="subscripts.DemoCalculatrice"/>
      <class name="subscripts.DemoTooltipCheck"/>
    </classes>
  </test>
</suite>