<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="toverify" verbose="10">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="2"/>
  <parameter name="ats.log.level" value="all"/>
  <test name="testMain" preserve-order="true">
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="scripting.scripting"/>
      <class name="special_features.check_null_versus_zero"/>
      <class name="special_features.random_input"/>
    </classes>
  </test>
</suite>