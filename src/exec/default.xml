<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="default" verbose="10">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="2"/>
  <parameter name="ats.log.level" value="all"/>
  <test name="testMain" preserve-order="true">
    <parameter name="var_env3" value="msedge"/>
    <parameter name="var_env2" value="chrome"/>
    <parameter name="android_ip_port" value="192.168.0.50:8081"/>
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="special_features.random_input"/>
      <class name="web.selection"/>
    </classes>
  </test>
</suite>