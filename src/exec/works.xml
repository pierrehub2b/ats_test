<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="works" verbose="10">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="3"/>
  <test name="testMain">
    <groups>
      <run>
        <exclude name="bug"/>
        <include name="default"/>
        <exclude name="toverify"/>
      </run>
    </groups>
    <packages>
      <package name=".*"/>
    </packages>
    <classes>
      <class name="AllScripts"/>
    </classes>
  </test>
</suite>