<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NoReg" verbose="10">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="2"/>
  <test name="testMain">
    <groups>
      <run>
        <include name="bug"/>
        <include name="default"/>
        <include name="web"/>
        <include name="desktop"/>
        <include name="api"/>
      </run>
    </groups>
    <packages>
      <package name=".*"/>
      <package name="api.*"/>
      <package name="channel.*"/>
      <package name="check.*"/>
      <package name="desktop.*"/>
      <package name="inputs.*"/>
      <package name="mouse.*"/>
      <package name="other.*"/>
      <package name="subscripts.*"/>
      <package name="subscripts.channel.*"/>
      <package name="subscripts.data.*"/>
      <package name="subscripts.dragdrop.*"/>
      <package name="subscripts.inputs.*"/>
      <package name="subscripts.mouse.*"/>
      <package name="subscripts.mouse.scroll.*"/>
      <package name="subscripts.window.*"/>
    </packages>
  </test>
</suite>